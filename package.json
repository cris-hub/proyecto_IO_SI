{
  "name": "magroplate_html",
  "version": "1.0.2",
  "description": "Custom boilerplate for HTML5 projects, includes Boostrap, jQuery, Gulp, Express, Brower-sync and more.",
  "main": "index.js",
  "scripts": {
    "test": "node index",
    "gitPush": "git push origin master && git push origin master --tags",
    "i_jQuery": "cp node_modules/jquery/dist/jquery.min.js www/js",
    "i_glyphicons": "cp -r node_modules/bootstrap-sass/assets/fonts/ www/fonts",
    "i_bootstrap": "cp node_modules/bootstrap-sass/assets/javascripts/bootstrap.min.js www/js",
    "compilar": "npm run i_jQuery && npm run i_bootstrap && npm run i_glyphicons",
    "runGulp": "gulp watch",
    "runSync": "browser-sync start --server 'www' --files 'www/**/*.html, www/js/*.js, www/css/*.css'",
    "start": "npm run runGulp & npm run runSync",
    "instalar": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmagrosoto/magroplate_html.git"
  },
  "keywords": [
    "fmagrosoto",
    "digitae",
    "boilerplate",
    "html5",
    "template"
  ],
  "author": "Fernando Magrosoto V. <fmagrosoto@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fmagrosoto/magroplate_html/issues"
  },
  "homepage": "https://github.com/fmagrosoto/magroplate_html#readme",
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^3.0.0",
    "jquery": "^3.2.1"
  }
}
